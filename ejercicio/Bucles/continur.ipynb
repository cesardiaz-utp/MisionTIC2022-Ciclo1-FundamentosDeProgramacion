    "variable = 10\n",
    "salto = []\n",
    "while variable > 0:\n",
    "    variable -= 1\n",
    "    if variable == 5:\n",
    "        salto.append(variable)\n",
    "        continue\n",
    "    print(f\"Actual valor de la variable: {variable}\")\n",
    "print('\\n', f'{5} es el numero que se salta con \"continue\"')"
   ]
